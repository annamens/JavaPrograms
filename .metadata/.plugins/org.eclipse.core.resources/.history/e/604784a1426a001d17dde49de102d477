package javaPrograms;

public class Demo {
	
	 static public void main(String ... args) {
		 
		 String s = "abba";
		 String temp=s;
		 String sum ="";
		 
		 for(int i=s.length()-1; i>=0;i--) {
			 sum = sum + s.charAt(i);
		 }
		 System.out.println(sum);
		 if(sum==temp) {
			 System.out.println("palindrome");
		 }
		 else {
			 System.out.println("not palindrome");
		 }
	 }


}
