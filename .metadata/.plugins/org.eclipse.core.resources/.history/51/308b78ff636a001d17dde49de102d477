package javaPrograms;

public class ArmstrongNumber {

	public static void main(String[] args) {
		int n = 123;
		int count = 0;
		while (n != 0) {
			n = n / 10;
			count++;
		}
		System.out.println(count);
		int sum=0;
		while(n!=0) {
			int x=n%10;
			x=()^count;
			sum = sum+x;
			n=n/10;
		}
		System.out.println(sum);
	}

}
