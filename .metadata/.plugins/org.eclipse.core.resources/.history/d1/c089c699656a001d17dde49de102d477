package javaPrograms;

public class ArmstrongNumber {

	public static void main(String[] args) {
		int n = 153;

		

//		int y = count;
		int z = 3;
		double sum = 0;
		while (n != 0) {
			double x = n % 10;

			x = Math.pow(x, z);
			sum = sum + x;
			System.out.println(x);
			n = n / 10;
		}
		System.out.println(sum);
	}

}
