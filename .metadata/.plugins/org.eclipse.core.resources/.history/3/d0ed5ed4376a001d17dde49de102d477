package javaPrograms;

class emp {

	int salary;
	String eid;
	static String ceo;

	static {
		ceo = "Ratan Tata";
	}

	public emp() {
		eid = "Mahesh";
		salary = 2000;
	}

	public void show() {
		System.out.println(eid + " : " + salary + " : " + ceo);
	}
}
public class StaticDemo {

	public static void main(String[] args) {
		emp mahesh = new emp();
		

	}

}
